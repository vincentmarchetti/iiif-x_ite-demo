{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "lib": [ "es2015", "dom" ],
        "moduleResolution": "node",
        "module": "es6",
        "outDir": "dist",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "skipLibCheck": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "target": "es6",
        
    },
    "include" : ["src/*.ts"]

}
